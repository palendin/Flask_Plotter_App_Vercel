{
  "version": 2,
  "builds": [
      {
          "src": "app.py",
          "use": "@vercel/python",
          "config": {
            "runtime": "python3.12",
            "pythonVersion": "3.12"
        }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "app.py"
      }
  ]
}