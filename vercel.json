{
  "version": 2,
  "builds": [
      {
          "src": "app.py",
          "use": "@vercel/python",
          "config": {
            "runtime": "python3.9",
            "buildImage": "legacy",
            "installCommand": "pip install pipenv && pipenv install --deploy --ignore-pipfile"
        }
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "app.py"
      }
  ]
}